{
    "name": "codehunter/smart-payment-router",
    "description": "A smart routing system for payment gateways in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulquadri Adegbiji",
            "email": "Adegbijilanre@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codehunter\\SmartPaymentRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codehunter\\SmartPaymentRouter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codehunter\\SmartPaymentRouter\\SmartPaymentRouterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}