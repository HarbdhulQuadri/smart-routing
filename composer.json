{
    "name": "harbdhulquadri/smart-routing",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/harbdhulquadri/smart-routing"
        }
    ],
    "description": "A smart payment routing system for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adegbiji Abdulquadri",
            "email": "adegbijiquadri@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Codehunter\\SmartPaymentRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codehunter\\SmartPaymentRouter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codehunter\\SmartPaymentRouter\\SmartPaymentRouterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}